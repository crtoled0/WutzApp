<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.wutz" version="2.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <feature name="Camera">
        <param name="browser-package" value="Camera" />
    </feature>
    <feature name="Device">
        <param name="browser-package" value="Device" />
    </feature>
    <name>Wutz</name>
    <description>
      App to connect to Wutz network and choose what you want to hear.
  </description>
    <author email="crtoledo@wutznet.com" href="http://wutz.co.uk">
      Cristian Toledo
  </author>
    <content src="index.html" />
    <icon src="www/icon.png" />
    <access origin="*" />
    <access origin="*.googleapis.com" />
    <access origin="*.gstatic.com" />
    <access origin="https://maps.google.com" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
</widget>
